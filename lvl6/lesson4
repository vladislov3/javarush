package com.javarush.task.pro.task04.task0413;

/* 
Рисуем треугольник
*/

public class Solution {
    public static void main(String[] args) {
        for(int i = 10; 1 <= i; i--) {
            int startCount = 11 - i;
            for(int j = 0; j < startCount; j++)
                System.out.print("8");
            System.out.println();

        }


    }
}
